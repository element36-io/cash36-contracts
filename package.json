{
  "name": "cash36-contracts",
  "version": "0.0.0-semantic-release",
  "description": "Contains Smart Contract JSON for cash36, generated by truffle",
  "homepage": "http://element36.io",
  "author": "element36",
  "contributors": [
    "m@element36.io"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./build && ./node_modules/.bin/truffle compile --all",
    "lint": "./node_modules/.bin/solium --dir contracts --reporter=pretty",
    "testrpc": "./node_modules/.bin/testrpc -p 8558 -i 85588558",
    "test": "./node_modules/.bin/truffle test --network local_testrpc",
    "test:coverage": "rm -rf ./coverage && rm -rf ./doc/coverage && SKIP_GAS_CHECKS=true ./node_modules/.bin/solidity-coverage && mv ./coverage ./doc && rm -rf ./doc/coverage/lcov-report",
    "oyente": "for f in ./contracts/*.sol; do python ~/element36/temp/oyente/oyente/oyente.py -s $f; done",
    "deploy:local": "./node_modules/.bin/truffle migrate --reset --compile-all --network local_parity",
    "deploy:testrpc": "./node_modules/.bin/truffle migrate --reset --compile-all --network local_testrpc",
    "deploy:dev": "./node_modules/.bin/truffle migrate --reset --compile-all --network dev",
    "generate-readme": "./scripts/generate-readme.js",
    "docs": "node_modules/.bin/doxity init && node_modules/.bin/doxity build && rm -rf ./scripts/doxity",
    "graph": "./scripts/runSolGraph.sh",
    "all": "yarn build && yarn lint && yarn deploy:testrpc && yarn test"
  },
  "dependencies": {
    "ethereumjs-testrpc": "6.0.3",
    "truffle": "^4.1.5",
    "openzeppelin-solidity": "1.9.0"
  },
  "devDependencies": {
    "@digix/doxity": "^0.5.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.23.0",
    "solgraph": "^0.2.6",
    "solidity-coverage": "^0.4.15",
    "solium": "^1.1.6"
  },
  "files": [
    "build/contracts/",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/cash36/contracts.git"
  },
  "main": "index.js"
}
